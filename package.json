    "@nestjs/platform-express": "^9.0.0",
    "@pgtyped/runtime": "^2.1.0",
    "@prisma/client": "4.14.1",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "environment-variables-decorator": "^1.0.5",
    "eventemitter2": "^6.4.9",
