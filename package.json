  "scripts": {
    "build": "yarn test && yarn clear && yarn compile",
    "compile": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "cli": "env MODE='CLI' nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "format:schema": "prisma format --schema src/engines/database/schema.prisma",
    "migrations:up": "prisma migrate dev --schema src/engines/database/schema.prisma",
    "generate:queries": "pgtyped -c pg-typed.config.json",
    "seed": "tsnd -r tsconfig-paths/register --transpile-only --ignore-watch node_modules src/scripts/seed.script.ts",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "clear": "rimraf dist",
    "commit": "cz"
  },
  "dependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^10.0.3",
