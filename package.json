    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.11.0",
    "pino": "^8.15.1",
    "pino-pretty": "^10.2.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@pgtyped/cli": "^2.2.1",
    "@types/bcrypt": "^5.0.0",
    "@types/chalk": "^2.2.0",
    "@types/express": "^4.17.13",
    "@types/inquirer": "^9.0.3",
    "@types/jest": "29.5.2",
